@model NoteApp.Models.Note

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Note Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NoteDescription)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NoteDescription)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateStart)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateStart)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateEnd)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateEnd)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NoteState)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NoteState)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NoteUserId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NoteUserId)
        </dd>
    </dl>
</div>
<div>
    <a class= "btn btn-warning" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> 
    <a class="btn btn-primary" asp-action="Index">Back to List</a>
</div>
